{
  "name": "angular2-socketio-chat-example",
  "version": "0.0.2",
  "description": "An example Angular 2 chat app, made on top of ExpressJS & Socket.io.",
  "homepage": "https://github.com/jussikinnula/",
  "repository": "https://github.com/jussikinnula/angular2-socketio-chat-example",
  "author": {
    "name": "Jussi Kinnula",
    "email": "jussi.kinnula@frantic.com"
  },
  "license": "GPL-3.0",
  "engines": {
    "node": ">=4.2"
  },
  "scripts": {
    "clean": "./node_modules/.bin/de ./target",
    "build:server": "./node_modules/.bin/webpack --config webpack.server.js",
    "build:client": "./node_modules/.bin/webpack --config webpack.client.js",
    "build": "npm run build:server && npm run build:client",
    "dev": "npm run build:server && ./node_modules/.bin/webpack-dev-server --config webpack.dev.js --progress",
    "start": "node target/assets/js/server.js",
    "postinstall": "./node_modules/.bin/typings install && npm run build"
  },
  "dependencies": {
    "angular2": "2.0.0-beta.15",
    "babel-polyfill": "^6.7.4",
    "css-loader": "~0.23.1",
    "es6-shim": "~0.35.0",
    "express": "^4.13.4",
    "extract-text-webpack-plugin": "~1.0.1",
    "html-webpack-plugin": "~2.15.0",
    "json-loader": "^0.5.4",
    "node-sass": "~3.4.2",
    "phantom": "^2.0.10",
    "postcss-loader": "~0.8.2",
    "raw-loader": "~0.5.1",
    "reflect-metadata": "0.1.3",
    "rxjs": "5.0.0-beta.6",
    "sass-loader": "~3.2.0",
    "serve-static": "^1.10.2",
    "style-loader": "~0.13.1",
    "ts-loader": "~0.8.2",
    "typescript": "~1.8.10",
    "typings": "~0.7.12",
    "webpack": "~1.12.15",
    "webpack-merge": "^0.10.0",
    "zone.js": "0.6.11"
  },
  "devDependencies": {
    "del": "~2.2.0",
    "del-cli": "~0.2.0",
    "webpack-dev-server": "^1.14.1",
    "webpack-livereload-plugin": "^0.8.1"
  }
}
